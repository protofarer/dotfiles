{
    "editor.rulers": [
    80,
    ],
    "editor.fontFamily": "hasklig, Source Code Pro, 'Courier New', monospace",
    "editor.find.cursorMoveOnType": false,
    "editor.defaultFormatter": null,
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "python.pythonPath": "c:\\Python39\\python.exe",
    "python.linting.pylintEnabled": true,
    "python.linting.flake8Enabled": false,
    "python.linting.enabled": true,
    "terminal.integrated.profiles.windows": {
        "Command Prompt": {
            "path": "C:\\WINDOWS\\System32\\cmd.exe",
            "args": []
        }
    },
    "sync.gist": "",
    "sync.autoDownload": false,
    "editor.renderWhitespace": "all",
    "editor.minimap.enabled": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "liveServer.settings.donotShowInfoMsg": true,
    "liveServer.settings.donotVerifyTags": true,
    "editor.fontLigatures": true,
    "editor.tabSize": 2,
    "python.defaultInterpreterPath": "python3",
    "editor.mouseWheelZoom": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "terminal.integrated.tabs.enabled": false,
    "files.autoSave": "afterDelay",
    "sync.autoUpload": false,
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.fontSize": 12,
    "livePreview.autoRefreshPreview": "On Changes to Saved Files",
    "workbench.colorTheme": "Gruvbox Dark Hard",
    "workbench.colorCustomizations": {
        "[Gruvbox Dark Hard]" : {
            "editor.background": "#000",
            "editor.lineHighlightBackground": "#333",
            "terminal.background": "#000",
            "sideBar.background": "#000",
            "minimap.background": "#000",
            // "panel.background": "#222",
            "statusBar.background": "#000",
            "tab.activeBackground": "#000",
            "tab.activeBorder": "#76e045",
            // "notifications.background": "#ff0000"
        }
    },
    "workbench.editor.labelFormat": "short",
    "explorer.confirmDragAndDrop": false,
    "editor.lineNumbers": "relative",
    "vim.vimrc.enable": true,
    "git.path": "/usr/bin/git",
    "gitlens.hovers.currentLine.over": "line",
    "remote.containers.dotfiles.repository": "https://github.com/protofarer/dotfilez",
    "remote.containers.dotfiles.targetPath": "~/.local/share/chezmoi/",
    "remote.containers.dotfiles.installCommand": "~/.local/share/chezmoi/install.sh",
    "dotfiles.targetPath": "",
    "terminal.integrated.defaultProfile.linux": "zsh",
    "vim.leader": "<space>",
    "vim.sneak": true,
    "vim.visualModeKeyBindings": [
        {
        "before": ["J"],
        "commands": ["editor.action.moveLinesDownAction"]
        },
        {
        "before": ["K"],
        "commands": ["editor.action.moveLinesUpAction"]
        }
    ],
    "emmet.includeLanguages": {
        "typescript": "typescriptreact",
        "javascript": "javascriptreact"
    },
    "workbench.activityBar.visible": false
}