{
	// Place your snippets for javascript here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	"Print to console.log": {
		"prefix": "clog",
		"body": [
			"console.log(`$1`, $2)",
			"$0"
		],
		"description": "browser stdout"
	},
	"Print to orange console.log": {
		"prefix": "cclog",
		"body": [
			"console.log(`%c$1`, 'color: orange', $2)",
			"$0"
		],
		"description": "browser stdout (color: orange)"
	},
	"Print to colored console.log": {
		"prefix": "dcclog",
		"body": [
			"console.log(`%c$1`, 'color: $2', $3)",
			"$0"
		],
		"description": "browser stdout (color: <color>)"
	},
	"Print to console.debug": {
		"prefix": "dlog",
		"body": [
			"console.debug(`$1`, $2)",
			"$0"
		],
		"description": "browser stderr"
	},
	"Print tag = 1": {
		"prefix": "cl1",
		"body": [
			"console.log(`%c1`, 'color: orange', $1)",
			"$0"
		],
		"description": "stdout colored tag 1"
	},
	"Print tag = 2": {
		"prefix": "cl2",
		"body": [
			"console.log(`%c2`, 'color: orange', $1)",
			"$0"
		],
		"description": "stdout colored tag 2"
	},
	"Print tag = 3": {
		"prefix": "cl3",
		"body": [
			"console.log(`%c3`, 'color: orange', $1)",
			"$0"
		],
		"description": "stdout colored tag 3"
	},
	"Start Code Block Header": {
		"prefix": "cblkstart",
		"body": [
			"$0// **********************************************************************",
			"$0// * ${1:codeBlockHeader}",
			"$0// **********************************************************************",
		],
		"description": "Add a prominent comment for a significant block of code"
	},
	"End Code Block Header": {
		"prefix": "cblkend",
		"body": [
			"$0// **********************************************************************",
			"$0// **********************************************************************",
		],
		"description": "End a prominent comment"
	}
}